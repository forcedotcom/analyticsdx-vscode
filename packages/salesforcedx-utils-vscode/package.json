{
  "name": "@salesforce/salesforcedx-utils-vscode",
  "displayName": "SFDX Utilities for VS Code",
  "description": "Provides utilies to interface the SFDX libraries with VS Code",
  "version": "0.3.0",
  "publisher": "salesforce",
  "license": "BSD-3-Clause",
  "dependencies": {
    "cross-spawn": "6.0.5",
    "path-exists": "3.0.0",
    "rxjs": "5.5.12",
    "tree-kill": "1.2.2"
  },
  "devDependencies": {
    "@types/chai": "^4.0.0",
    "@types/cross-spawn": "6.0.0",
    "@types/glob": "^7.1.1",
    "@types/mocha": "7.0.1",
    "@types/node": "12.12.18",
    "@types/path-exists": "^1.0.29",
    "chai": "^4.0.2",
    "decache": "^4.1.0",
    "glob": "^7.1.2",
    "istanbul": "^0.4.5",
    "mocha": "7.0.1",
    "mocha-junit-reporter": "^1.13.0",
    "mocha-multi-reporters": "^1.1.4",
    "mock-spawn": "0.2.6",
    "nyc": "^15.0.0",
    "remap-istanbul": "^0.9.5",
    "request-light": "0.2.4",
    "shx": "0.3.2",
    "source-map-support": "^0.5.16",
    "typescript": "3.7.5"
  },
  "scripts": {
    "compile": "webpack",
    "lint": "tslint --project .",
    "watch": "tsc -watch -p .",
    "clean": "shx rm -rf out && shx rm -rf coverage && shx rm -rf .nyc_output && shx rm -rf node_modules",
    "test": "npm run test:unit",
    "test:unit": "./node_modules/.bin/nyc ./node_modules/.bin/_mocha --recursive out/test/unit --reporter mocha-multi-reporters --timeout 20000 --reporter mocha-multi-reporters --reporter-options configFile=../../config/mochaUnitTestsConfig.json"
  },
  "nyc": {
    "reporter": [
      "text-summary"
    ]
  }
}
