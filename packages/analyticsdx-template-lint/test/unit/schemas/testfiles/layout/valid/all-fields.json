            },
            {
              "type": "Variable",
              "name": "selectedNumber",
              "visibility": "{{Variables.booleanVar ? 'Visible' : 'Hidden'}}"
            },
            {
              "type": "GroupBox",
              "text": "test groupbox header",
              "description": "test groupbox header description",
              "items": [
                {
                  "type": "Image",
                  "visibility": "{{Variables.booleanVar ? 'Visible' : 'Hidden'}}",
