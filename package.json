{
  "devDependencies": {
    "@salesforce/dev-config": "1.4.1",
    "@types/mkdirp": "0.5.2",
    "eslint": "4.13.1",
    "eslint-config-prettier": "2.9.0",
    "istanbul": "1.1.0-alpha.1",
    "lerna": "3.13.1",
    "mkdirp": "0.5.1",
    "prettier": "1.16.4",
    "remap-istanbul": "^0.9.5",
    "shx": "0.2.2",
    "snyk": "^1.82.1",
    "ts-loader": "5.4.5",
    "tslint": "5.5.0",
    "tslint-no-focused-test": "0.5.0",
    "typescript": "3.5.2",
    "vsce": "1.62.0",
    "vscode-test": "1.0.0",
    "webpack": "4.30.0",
    "webpack-cli": "3.3.1"
  },
  "scripts": {
    "postinstall": "lerna bootstrap -- --no-package-lock && node scripts/reformat-with-prettier",
    "bootstrap": "lerna bootstrap -- --no-package-lock && node scripts/reformat-with-prettier",
    "clean": "lerna run clean",
    "compile": "lerna run --stream compile",
    "lint": "lerna run lint",
    "test": "lerna exec --concurrency 1 --stream --bail=false -- npm run test",
    "test:unit": "lerna exec --concurrency 1 --stream --bail=false -- npm run test:unit",
    "test:vscode-integration": "lerna exec --concurrency 1 --stream --bail=false -- npm run test:vscode-integration",
    "test:vscode-insiders-integration": "lerna exec --concurrency 1 --stream --bail=false -- npm run test:vscode-insiders-integration",
    "vscode:package": "lerna exec --scope @salesforce/salesforcedx-utils-vscode -- npm prune --production && lerna run vscode:package --concurrency 1 && node scripts/reformat-with-prettier",
    "vscode:sha256": "lerna run vscode:sha256 --concurrency 1",
    "vscode:publish": "lerna run vscode:publish --concurrency 1",
    "watch": "lerna run --parallel watch",
    "eslint-check": "eslint --print-config .eslintrc.json | eslint-config-prettier-check",
    "reformat": "node scripts/reformat-with-prettier.js",
    "snyk:test": "lerna exec --bail=false --ignore analyticsdx-vscode -- snyk test --severity-threshold=medium --show-vulnerable-paths=false",
    "snyk:monitor": "lerna exec --bail=false --ignore analyticsdx-vscode -- snyk monitor --severity-threshold=medium --show-vulnerable-paths=false --org=vazexqi",
    "aggregateJUnit": "node scripts/aggregate-junit-xml.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://git.soma.salesforce.com/ADX/analyticsdx-vscode.git"
  },
  "license": "BSD-3-Clause",
  "dependencies": {
    "node": "10.11.0"
  }
}
